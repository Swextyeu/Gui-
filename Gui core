local targetTitle = Instance.new("TextLabel")
targetTitle.Size = UDim2.new(1, -30, 0, 30)
targetTitle.Position = UDim2.new(0, 15, 0, 5)
targetTitle.BackgroundTransparency = 1
targetTitle.Text = "TARGET SELECTION"
