    tab1.BackgroundColor3 = settings.accentColor
    tab2.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
    tab1.TextColor3 = Color3.fromRGB(255, 255, 255)
    tab2.TextColor3 = Color3.fromRGB(180, 180, 180)
end)

tab2.MouseButton1Click:Connect(function()
    tab2.BackgroundColor3 = settings.accentColor
    tab1.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
    tab2.TextColor3 = Color3.fromRGB(255, 255, 255)
    tab1.TextColor3 = Color3.fromRGB(180, 180, 180)
end)

-- Enabled toggle
local enabledFrame = Instance.new("Frame")
