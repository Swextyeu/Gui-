-- Status grid
local statusGrid = Instance.new("Frame")
statusGrid.Size = UDim2.new(1, -20, 0, 110)
statusGrid.Position = UDim2.new(0, 10, 0, 185)
statusGrid.BackgroundTransparency = 1
statusGrid.Parent = mainFrame

local statusTexts = {
    {text = "Successfully", color = Color3.fromRGB(120, 40, 200)},
    {text = "Warning", color = Color3.fromRGB(200, 180, 0)},
    {text = "Visuals", color = Color3.fromRGB(120, 40, 200)},
    {text = "Silent Aim", color = Color3.fromRGB(120, 40, 200)},
    {text = "None", color = Color3.fromRGB(150, 150, 150)},
    {text = "Error", color = Color3.fromRGB(200, 50, 50)},
    {text = "Info", color = Color3.fromRGB(120, 40, 200)}
}

for i, data in ipairs(statusTexts) do
    local row = math.floor((i-1) / 3)
    local col = (i-1) % 3
    
    local btn = Instance.new("TextButton")
    btn.Name = data.text.."Button"
    btn.Size = UDim2.new(0, 110, 0, 30)
    btn.Position = UDim2.new(0, col * 115, 0, row * 35)
    btn.BackgroundColor3 = data.color
    btn.BackgroundTransparency = data.text == "None" and 0.5 or 0
    btn.Text = data.text
    btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    btn.TextSize = 12
    btn.Font = Enum.Font.GothamBold
    btn.BorderSizePixel = 0
    btn.Parent = statusGrid
    
    btn.MouseButton1Click:Connect(function()
        print("Clicked:", data.text)
        -- Visual feedback
        btn.BackgroundTransparency = 0
        task.wait(0.1)
        btn.BackgroundTransparency = data.text == "None" and 0.5 or 0
    end)
end

-- Bottom buttons (Skins, Config, Misc)
local bottomButtons = {"Skins", "Config", "Misc"}
for i, text in ipairs(bottomButtons) do
    local btn = Instance.new("TextButton")
