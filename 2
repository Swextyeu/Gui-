-- Accent Color
local accentFrame = Instance.new("Frame")
accentFrame.Size = UDim2.new(0, 160, 0, 35)
accentFrame.Position = UDim2.new(0, 180, 0, 455)
accentFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
accentFrame.BorderSizePixel = 0
accentFrame.Parent = mainFrame

local accentLabel = Instance.new("TextLabel")
accentLabel.Size = UDim2.new(0, 90, 1, 0)
accentLabel.Position = UDim2.new(0, 10, 0, 0)
accentLabel.BackgroundTransparency = 1
accentLabel.Text = "Accent Color"
accentLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
accentLabel.TextXAlignment = Enum.TextXAlignment.Left
accentLabel.Font = Enum.Font.Gotham
accentLabel.TextSize = 14
accentLabel.Parent = accentFrame

local accentBox = Instance.new("TextButton")
accentBox.Size = UDim2.new(0, 25, 0, 25)
accentBox.Position = UDim2.new(0, 110, 0, 5)
accentBox.BackgroundColor3 = settings.accentColor
accentBox.BorderSizePixel = 0
accentBox.Text = ""
accentBox.Parent = accentFrame

local colors = {
    Color3.fromRGB(120, 40, 200),  -- Purple
    Color3.fromRGB(200, 40, 40),   -- Red
    Color3.fromRGB(40, 120, 200),  -- Blue
    Color3.fromRGB(40, 200, 40),   -- Green
    Color3.fromRGB(200, 120, 40)   -- Orange
}
local currentColor = 1
accentBox.MouseButton1Click:Connect(function()
    currentColor = currentColor % #colors + 1
    settings.accentColor = colors[currentColor]
    accentBox.BackgroundColor3 = settings.accentColor
    
    -- Update all accent colored elements
    enabledToggle.BackgroundColor3 = settings.enabled and settings.accentColor or Color3.fromRGB(80, 80, 80)
    tab1.BackgroundColor3 = settings.accentColor
    for _, btn in ipairs(categoryButtons) do
        if btn.Text == "Enemy" then
            btn.BackgroundColor3 = settings.accentColor
        end
    end
    print("Accent color changed")
end)

