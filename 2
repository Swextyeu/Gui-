local enabledToggle = Instance.new("TextButton")
enabledToggle.Size = UDim2.new(0, 45, 0, 25)
enabledToggle.Position = UDim2.new(0, 95, 0, 5)
enabledToggle.BackgroundColor3 = settings.accentColor
enabledToggle.Text = "ON"
enabledToggle.TextColor3 = Color3.fromRGB(255, 255, 255)
enabledToggle.TextSize = 12
enabledToggle.Font = Enum.Font.GothamBold
enabledToggle.BorderSizePixel = 0
enabledToggle.Parent = enabledFrame

enabledToggle.MouseButton1Click:Connect(function()
    settings.enabled = not settings.enabled
    if settings.enabled then
        enabledToggle.BackgroundColor3 = settings.accentColor
        enabledToggle.Text = "ON"
    else
        enabledToggle.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
        enabledToggle.Text = "OFF"
    end
    print("Enabled:", settings.enabled)
end)

